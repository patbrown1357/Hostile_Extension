{
    "name": "Hostile Extension",
    "description": "a mean extension",
    "version": "1.0",
    "icons": {
        "16" :"icon.PNG",
        "48": "icon.PNG",
        "128": "icon.PNG"
    },
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css":["styles.css"],
            "js": ["jquery-3.5.1.min.js","scripts/content.js", "scripts/changer.js"],
            "run_at":"document_end"
        }
    ],
    "browser_action": {
        "default_icon":"icon.PNG",
        "default_popup":"popup.html"
    },
    "background": {
        "scripts": ["scripts/background.js"]
    },
    "permissions": [
        "tabs", "fontSettings"
    ]
}